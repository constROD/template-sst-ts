    "pg": "^8.10.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.1",
    "@types/pg": "^8.6.6",
    "eslint-config-base": "workspace:*",
    "prisma": "^4.14.0",
    "sst": "^2.7.2",
    "vitest": "^0.30.1"
  }
